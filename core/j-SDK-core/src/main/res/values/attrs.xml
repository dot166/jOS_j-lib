<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="jToolbar">
        <!-- Whether the activity icon corresponding to the "android:icon" attribute
             should be used as the logo within the toolbar. Default is true.
        -->
        <attr name="iconAsLogo" format="boolean" />
        <!-- Whether the subtitle text corresponding to the "app:subtitle" attribute
             should be centered horizontally within the toolbar. Default is false.
        -->
        <attr name="jsubtitleCentered" format="boolean" />
    </declare-styleable>
</resources>