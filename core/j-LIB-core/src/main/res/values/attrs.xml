<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActionBar2">
        <!-- Whether the activity icon corresponding to the "android:icon" attribute
             should be used as the logo within the toolbar. Default is true.
        -->
        <attr name="iconAsLogo" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MenuButton">
        <attr name="android:text" />
        <attr name="android:icon" />
        <attr name="tint" />
    </declare-styleable>

    <declare-styleable name="ThemeEngine">
        <attr name="isJTheme" format="boolean" />
    </declare-styleable>
</resources>