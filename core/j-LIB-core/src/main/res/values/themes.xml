        <item name="materialCalendarFullscreenTheme">@style/ThemeOverlay.MaterialComponents.MaterialCalendar.Fullscreen</item>
        <item name="materialCalendarTheme">@style/ThemeOverlay.MaterialComponents.MaterialCalendar</item>
        <item name="materialTimePickerTheme">@style/ThemeOverlay.MaterialComponents.TimePicker</item>

        <!-- AndroidX Preference Fixes -->
        <item name="preferenceTheme">@style/preference_j</item>
    </style>
    <!-- Theme.Holo.NoActionbar overrides -->
    <style name="Platform.jOS.Theme" parent="android:Theme.Holo.NoActionBar"/>
