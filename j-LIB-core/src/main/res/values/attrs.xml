<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActionBar2">
        <!-- Whether the activity icon corresponding to the "android:icon" attribute
             should be used as the logo within the toolbar. Default is true.
        -->
        <attr name="iconAsLogo" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MenuButton">
        <attr name="android:text" />
        <attr name="android:icon" />
        <attr name="tint" />
    </declare-styleable>

    <declare-styleable name="ThemeEngine">
        <attr name="isJTheme" format="boolean" />
    </declare-styleable>
    <!-- Base attributes available to SeekBarPreference. -->
    <declare-styleable name="jLibSeekBarPreference">
        <attr name="defaultValueText" format="string" />
        <attr name="interval" format="integer" />
        <attr name="showSign" format="boolean" />
        <attr name="units" format="string|reference" />
        <attr name="continuousUpdates" format="boolean" />
        <attr name="android:max"/>
        <attr name="android:min"/>
        <attr name="android:defaultValue"/>
    </declare-styleable>
</resources>